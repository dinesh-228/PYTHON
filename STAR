b=int(input("Enter no of blocks : "))
l=int(input("Enter no of lines : "))
s=int(input("Enter no of star's : "))
for i in range(b):
    c=0
    for j in range(l-i):
        for k in range(s):
            print("*", end="")
            c+=1
        print()
    print(c)
    print("_________________")
